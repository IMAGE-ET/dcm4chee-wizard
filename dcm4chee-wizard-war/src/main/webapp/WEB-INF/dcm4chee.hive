grant principal org.wicketstuff.security.hive.authorization.SimplePrincipal "Advanced"
{
// KEY: swarm.principal.Advanced
	permission ${ComponentPermission} "org.dcm4chee.wizard.panel.AdvancedConfigurationPanel", "render, enable";
};

grant principal org.wicketstuff.security.hive.authorization.SimplePrincipal "Other"
{
// KEY: swarm.principal.Other
	permission ${ComponentPermission} "org.dcm4chee.wizard.panel.WizardPanel", "render, enable";
    permission ${ComponentPermission} "org.dcm4chee.wizard.panel.ProfilePanel", "render, enable";
    permission ${ComponentPermission} "org.dcm4chee.wizard.panel.DicomConfigurationSourcePanel", "render, enable";
};
